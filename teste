    window.onscroll = function() {
    const barraInf = document.querySelector(".barraInf");
    if (window.pageYOffset > barraInf.offsetTop) {
      barraInf.classList.add("sticky");
    } else {
      barraInf.classList.remove("sticky");
    }
  };
  </script>
